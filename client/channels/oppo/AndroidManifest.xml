        <!-- 银联配置 -->


        <!-- 供第三方使用的 Service 、Receiver的配置 -->
        <receiver android:name="com.nearme.wappay.open.ThirdShareReceiver" >
            <intent-filter android:priority="2147483647" >
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
            <intent-filter>

                <!-- WIFI开关改变 -->
                <action android:name="android.net.wifi.WIFI_STATE_CHANGED" />
                <!-- WIFI连接改变 -->
                <action android:name="android.net.wifi.STATE_CHANGE" />
                <!-- SIM卡状态改变 -->
                <action android:name="android.intent.action.SIM_STATE_CHANGED" />
                <!-- 网络状态改变 -->
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
                <!-- 用户解锁状态 -->
                <action android:name="android.intent.action.USER_PRESENT" />
                <!-- 手机状态改变 -->
                <action android:name="android.intent.action.PHONE_STATE" />
                <!-- 日期改变广播 -->
                <action android:name="android.intent.action.DATE_CHANGED" />
                <!-- 时区改变广播 -->
                <action android:name="android.intent.action.TIMEZONE_CHANGED" />
                <action android:name="android.intent.action.MAIN" />
                <action android:name="com.ehoo.action.SEND" />
            </intent-filter>
        </receiver>

        <service android:name="com.nearme.wappay.open.ThirdShareService" >
        </service>

        <!-- Upay 支付专用的Receiver设置 -->
        <receiver android:name="com.youge.upay_sms_sdk_demo.UpaySentReceiver" >
        </receiver>
