<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content">
	<LinearLayout android:id="@+id/yk_pay_params_layout"
	    android:layout_width="@dimen/yk_xh_664px"
		android:layout_height="wrap_content" 
	    android:background="#FfFfFf"
		android:orientation="vertical"
		android:paddingBottom="@dimen/yk_xh_30px">
	<RelativeLayout
		android:layout_width="match_parent"
    	android:layout_height="@dimen/yk_xh_88px"
		android:orientation="horizontal">
     	<ImageView android:id="@+id/yk_pay_icon_back"
		   	android:layout_width="wrap_content"
	     	android:layout_height="wrap_content"
		    android:src="@drawable/yk_pay_back"
		    android:paddingLeft="@dimen/yk_xh_36px"
		    android:paddingRight="@dimen/yk_xh_36px"
		    android:layout_centerVertical="true"/>
		<TextView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:layout_gravity="center_vertical"
			android:textSize="@dimen/yk_xh_44px"
			android:textColor="@color/text_title"
			android:layout_toRightOf="@+id/yk_pay_icon_back"
			android:layout_marginLeft="@dimen/yk_xh_26px"
			android:text="@string/yk_text_ykgame_pay"
			android:layout_centerVertical="true"/>
		<ImageView android:id="@+id/yk_pay_icon_detail"
	   		android:layout_width="wrap_content"
     	    android:layout_height="wrap_content"
	    	android:src="@drawable/yk_pay_detail"
		    android:paddingLeft="@dimen/yk_xh_26px"
		    android:paddingRight="@dimen/yk_xh_36px"
		    android:paddingTop="@dimen/yk_xh_22px"
		    android:paddingBottom="@dimen/yk_xh_22px"
	    	android:layout_alignParentRight="true"
	    	android:layout_centerVertical="true"/>
	</RelativeLayout>
	<LinearLayout 
	    android:layout_height="1px"
	    android:layout_width="fill_parent"
	    android:background="@color/title_line">
	</LinearLayout>
	<LinearLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="@dimen/yk_xh_18px">
	</LinearLayout>
	<RelativeLayout
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal">
		<ScrollView android:id="@+id/yk_paylist_scroll"
		    android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:scrollbars="none">
	    <LinearLayout android:id="@+id/yk_pay_paylist_layout"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:orientation="vertical">
	        <RelativeLayout android:id="@+id/yk_pay_select_alipay_bt"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal">
		        <ImageView android:id="@+id/yk_pay_alipay_arrow_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_arrow_blue"/>
		        <ImageView android:id="@+id/yk_pay_alipay_logo_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_logo_alipay"/>
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/yk_pay_select_operator_bt"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        android:paddingTop="@dimen/yk_xh_14px">
		        <ImageView android:id="@+id/yk_pay_operator_arrow_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_arrow_blue"/>
		        <ImageView android:id="@+id/yk_pay_operator_logo_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_logo_operator"/>
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/yk_pay_select_sms_bt"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        android:paddingTop="@dimen/yk_xh_14px">
		        <ImageView android:id="@+id/yk_pay_sms_arrow_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_arrow_blue"/>
		        <ImageView android:id="@+id/yk_pay_sms_logo_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_logo_sms"/>
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/yk_pay_select_unionpay_bt"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        android:paddingTop="@dimen/yk_xh_14px"
		        android:visibility="gone">
		        <ImageView android:id="@+id/yk_pay_unionpay_arrow_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_arrow_blue"/>
		        <ImageView android:id="@+id/yk_pay_unionpay_logo_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_logo_unionpay"
		            />
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/yk_pay_select_tenpay_bt"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        android:paddingTop="@dimen/yk_xh_14px">
		        <ImageView android:id="@+id/yk_pay_tenpay_arrow_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_arrow_blue"/>
		        <ImageView android:id="@+id/yk_pay_tenpay_logo_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_logo_tenpay"
		            />
	        </RelativeLayout>
	        <RelativeLayout android:id="@+id/yk_pay_select_gold_bt"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:orientation="horizontal"
		        android:paddingTop="@dimen/yk_xh_14px">
		        <ImageView android:id="@+id/yk_pay_gold_arrow_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_arrow_blue"/>
		        <ImageView android:id="@+id/yk_pay_gold_logo_img"
		            android:layout_width="@dimen/yk_xh_150px"
		            android:layout_height="@dimen/yk_xh_75px"
		            android:background="@drawable/yk_pay_logo_gold"/>
	        </RelativeLayout>
	    </LinearLayout>
	    </ScrollView>
	    <ImageView android:id="@+id/yk_pay_paylist_vertical_cut"
	        android:layout_width="1px"
		   	android:layout_height="@dimen/yk_xh_474px"
		   	android:layout_toRightOf="@+id/yk_paylist_scroll"
		   	android:background="#c1c1c1"/>
	    <include android:id="@+id/yk_pay_goldrecha_include"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			layout="@layout/dialog_yk_pay_goldrecha_include"
			android:layout_marginLeft="@dimen/yk_xh_44px"
	        android:layout_marginRight="@dimen/yk_xh_44px"
			android:layout_toRightOf="@+id/yk_pay_paylist_vertical_cut"/>
	    <ScrollView android:id="@+id/yk_pay_content_scroll"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:scrollbars="none"
	        android:layout_marginLeft="@dimen/yk_xh_44px"
	        android:layout_marginRight="@dimen/yk_xh_44px"
	        android:layout_toRightOf="@+id/yk_pay_paylist_vertical_cut">
	    <RelativeLayout android:id="@+id/yk_pay_content_layout1"
	        android:layout_width="wrap_content"
		   	android:layout_height="wrap_content"
	        android:orientation="vertical"
	        >
	        
	        <RelativeLayout android:id="@+id/yk_pay_content_layout2"
	        	android:layout_width="wrap_content"
		   		android:layout_height="wrap_content"
		   		android:orientation="horizontal"
		   		android:layout_marginBottom="@dimen/yk_xh_10px">
		        <TextView android:id="@+id/yk_pay_amount_txt"
		            android:layout_width="wrap_content"
		   			android:layout_height="wrap_content"
		   			android:text="@string/yk_text_000.00"
		   			android:textColor="#ff9c00"
		   			android:textSize="@dimen/yk_xh_44px"
		   			android:layout_centerVertical="true"
		   			android:layout_marginRight="@dimen/yk_xh_24px"/>
		        <ImageView android:id="@+id/yk_pay_amount_cut"
		            android:layout_width="1px"
		   			android:layout_height="@dimen/yk_xh_70px"
		   			android:background="#c1c1c1"
		   			android:layout_centerVertical="true"
		   			android:layout_toRightOf="@+id/yk_pay_amount_txt"/>
		        
		        <LinearLayout android:id="@+id/yk_pay_content_layout3"
		            android:layout_width="wrap_content"
		   			android:layout_height="wrap_content"
		   			android:orientation="vertical"
		   			android:layout_centerVertical="true"
		   			android:layout_toRightOf="@+id/yk_pay_amount_cut"
		   			android:layout_marginLeft="@dimen/yk_xh_24px">
		           	<LinearLayout android:id="@+id/yk_pay_user_layout"
		           	    android:orientation="horizontal"
		           	    android:layout_width="wrap_content"
		   				android:layout_height="wrap_content">
		   				<TextView android:text="@string/yk_text_user_maohao"
		   				    android:layout_width="wrap_content"
		   					android:layout_height="wrap_content"
		   					android:textColor="#b3b3b3"
		   					android:textSize="@dimen/yk_xh_26px"/>
		   				<TextView android:id="@+id/yk_pay_user_txt"
		   				    android:layout_width="wrap_content"
		   					android:layout_height="wrap_content"
		   				    android:text="@string/yk_text_null"
		   				    android:singleLine="true"
		   					android:textColor="#717171"
		   					android:textSize="@dimen/yk_xh_26px"/>
		           	</LinearLayout>
		           	<LinearLayout android:orientation="horizontal"
		           	    android:layout_width="wrap_content"
		   				android:layout_height="wrap_content">
		   				<TextView 
		   				    android:layout_width="wrap_content"
		   					android:layout_height="wrap_content"
		   					android:text="@string/yk_text_good_maohao"
		   					android:textColor="#b3b3b3"
		   					android:textSize="@dimen/yk_xh_26px"/>
		   				<TextView android:id="@+id/yk_pay_product_txt"
		   				    android:layout_width="wrap_content"
		   					android:layout_height="wrap_content"
		   					android:text="@string/yk_text_0xingzuan"
		   					android:singleLine="true"
		   					android:textColor="#717171"
		   					android:textSize="@dimen/yk_xh_26px"/>
		           	</LinearLayout>
		        </LinearLayout>
						
	            
	        </RelativeLayout>
	        <ImageView android:id="@+id/yk_pay_cutingline"
	            android:layout_width="@dimen/yk_xh_474px"
	            android:layout_height="1px"
	            android:background="#c1c1c1"
	            android:layout_below="@+id/yk_pay_content_layout2"/>
		    <include android:id="@+id/yk_pay_include"
		        android:layout_width="wrap_content"
			   	android:layout_height="wrap_content"
		        android:orientation="vertical"
		        layout="@layout/dialog_yk_pay_sms_include"
		        android:layout_below="@+id/yk_pay_cutingline"
		        android:layout_marginTop="@dimen/yk_xh_36px"/>
		    <include android:id="@+id/yk_pay_operator_include"
		        android:layout_width="wrap_content"
			   	android:layout_height="wrap_content"
		        android:orientation="vertical"
		        layout="@layout/dialog_yk_pay_operator_include"
		        android:layout_below="@+id/yk_pay_content_layout2"
		        android:layout_marginTop="@dimen/yk_xh_14px"/>
		    <include android:id="@+id/yk_pay_gold_include"
		        android:layout_width="wrap_content"
			   	android:layout_height="wrap_content"
		        android:orientation="vertical"
		        layout="@layout/dialog_yk_pay_gold_include"
		        android:layout_below="@+id/yk_pay_content_layout2"/>
	 		<TextView android:id="@+id/yk_pay_alipay_bt"
			    android:layout_width="@dimen/yk_xh_474px"
			    android:layout_height="@dimen/yk_xh_88px"
			    android:background="@drawable/yk_btn_blue_selector"
			    android:text="@string/yk_text_a_qzfbfk"
			    android:textSize="@dimen/yk_xh_36px"
			    android:textColor="#ffffff"
			    android:gravity="center"
			    android:layout_below="@+id/yk_pay_operator_include"
			    android:layout_marginTop="@dimen/yk_xh_24px"/>
	 		<RelativeLayout android:id="@+id/yk_pay_operator_tip_layout"
	 		    android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_below="@+id/yk_pay_alipay_bt"
				android:layout_marginTop="@dimen/yk_xh_20px">
		 		<TextView android:id="@+id/yk_pay_operator_tip"
				    android:layout_width="fill_parent"
				    android:layout_height="wrap_content"
				    android:text="@string/yk_text_prompt_xzmzxtczk"
				    android:textSize="@dimen/yk_xh_24px"
				    android:textColor="@color/edit_text_hint"/>
		 		<TextView android:id="@+id/yk_pay_operator_tip_link"
		 		    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:text="@string/yk_text_prompt_ljgd"
				    android:textSize="@dimen/yk_xh_26px"
				    android:textColor="@drawable/yk_tv_hink_find_pwd_selector"
				    android:layout_alignParentRight="true"
				    android:layout_alignBottom="@+id/yk_pay_operator_tip"/>
	 		</RelativeLayout>
	    </RelativeLayout>
	    </ScrollView>
	</RelativeLayout>
	</LinearLayout>
</LinearLayout>